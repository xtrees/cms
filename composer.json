{
    "name": "xtrees/cms",
    "description": "cms toolkit for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "xtrees",
            "email": "xutao58@qq.com",
            "homepage": ""
        }
    ],
    "homepage": "https://github.com/xtrees/cms",
    "keywords": ["Laravel", "CMS"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "autoload": {
        "psr-4": {
            "XTrees\\CMS\\": "src",
            "XTrees\\CMS\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "XTrees\\CMS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XTrees\\CMS\\CMSServiceProvider"
            ],
            "aliases": {
                "CMS": "XTrees\\CMS\\Facades\\CMS"
            }
        }
    }
}
